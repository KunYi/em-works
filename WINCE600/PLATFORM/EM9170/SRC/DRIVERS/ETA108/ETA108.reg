

[HKEY_LOCAL_MACHINE\Drivers\BuiltIn\ADS1]
   "Prefix"="ADS"									 
   "Dll"="ETA108.dll"								 
   "Index"		 = dword:1		;"ADS1:"
   "CSPIChannel" = dword:1      ; use CSPI1
   "PWMChannel"  = dword:1      ; use PWM1
   "DMABufSize"	 = dword:5120	; DMA buffer size
   "MultDMABufSize" = dword:20	; Buffer Size of Continuous Sampling 
   


