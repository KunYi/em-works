TARGETNAME=oal_timer32k_mxarm11
TARGETTYPE=LIBRARY
SYNCHRONIZE_DRAIN=1
NOMIPS16CODE=1

SOURCELIBS= \
    $(_PLATCOMMONLIB)\$(_CPUDEPPATH)\oal_timer.lib

INCLUDES=\
    $(INCLUDES); \
    ..\..\..\common_fsl_v1\inc; \
    ..\..\inc

WARNLEVEL=$(BSP_WARNLEVEL)

CDEFINES=$(CDEFINES) -DCEDDK_USEDDKMACRO -DOAL_CLOCK_CALIBRATION=30

!IF "$(ENABLE_WATCH_DOG)"=="1"
CDEFINES=$(CDEFINES) -DENABLE_WATCH_DOG
!ENDIF

!IF "$(_TGTBOARD)" != ""
CDEFINES=$(CDEFINES) -D$(_TGTBOARD)
!ENDIF

SOURCES= \
    timer.c
