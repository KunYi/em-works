!IF "$(BSP_SDHC1)$(BSP_SDHC2)" == ""
SKIPBUILD=1
!ENDIF

!IF "$(BSP_HIVE_SDHC)" == "1"
CDEFINES=$(CDEFINES) -DBSP_HIVE_SDHC
!ENDIF

CDEFINES=$(CDEFINES) -DMSCDSK

