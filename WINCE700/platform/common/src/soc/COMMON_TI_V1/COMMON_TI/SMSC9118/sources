!INCLUDE "..\sources.inc"

!IF "$(BSP_SMSC911X)" == ""
SKIPBUILD=1
!ENDIF

TARGETNAME=smsc9118
TARGETTYPE=LIBRARY

PLAT_SOURCES=platform.c util.S
CDEFINES=$(CDEFINES)

!IF "$(WINCEDEBUG)" == "debug"
CDEFINES=$(CDEFINES) -DSMSC_DBG
!ENDIF


TARGETLIBS=	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
		  	$(_PLATLIB)\$(_CPUDEPPATH)\ceddk.lib \
		  	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\ndis.lib \
		  	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\winsock.lib \

SOURCES=		\
	lan9118.c	\
	os.c		\
	smsc9118.c	\
	$(PLAT_SOURCES)
	
INCLUDES=$(INCLUDES);$(_TARGETPLATROOT)\src\inc

